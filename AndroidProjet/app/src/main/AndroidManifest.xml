<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cutMonster.androidprojet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidProjet"
        android:configChanges="orientation|screenSize">

        <activity android:name="com.cutMonster.androidprojet.view.AccueilActivity" android:theme="@style/Theme.AndroidProjet.noActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.cutMonster.androidprojet.view.JeuActivity" android:theme="@style/Theme.AndroidProjet.noActionBar" android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.cutMonster.androidprojet.view.ScoreActivity" android:theme="@style/Theme.AndroidProjet.noActionBar">
        </activity>

    </application>


</manifest>